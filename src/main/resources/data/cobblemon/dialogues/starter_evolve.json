{
  "escapeAction": "q.dialogue.set_page('farewell')",
  "speakers": {
    "professor": {
      "name": "Professor Willow",
      "face": {
        "type": "artificial",
        "modelType": "pokemon",
        "identifier": "cobblemon:ditto",
        "isLeftSide": false,
        "aspects": []
      }
    },
    "player": {
      "face": "q.player.face(true);",
      "name": {
        "type": "expression",
        "expression": "q.player.username"
      }
    }
  },
  "pages": [
    {
      "id": "intro",
      "speaker": "professor",
      "lines": [
        {
          "type": "expression",
          "expression": "'Ah, ' + q.player.username + '! Just the trainer I was looking for.'"
        },
        "Do you have the TNT?"
      ],
      "input": {
        "type": "option",
        "options": [
          {
            "text": "Yes! Here you go.",
            "isSelectable": "q.player.has_item('minecraft:tnt', 1)",
            "action": [
              "q.player.execute_command('journey completesubtask {player} journey:find_professor journey:find_professor');",
              "q.dialogue.close()"
            ]
          },
          {
            "text": "No, sorry!",
            "action": [
              "q.dialogue.close()"
            ]
          }
        ]
      }
    },
    {
      "id": "farewell",
      "speaker": "professor",
      "lines": [
        "Goodbye!"
      ],
      "input": {
        "type": "none"
      }
    }
  ]
}
